.end_game
	.col-lg-6.col-lg-offset-3.col-md-6.col-md-offset-3.col-xs-12.col-sm-12
		%header
			.jumbotron
				.container
					%h1
						='Game finished'
					-if @gana
						%p='You Win'
					-else
						%p='You Lose'
			%button.btn.btn-primary.btn-lg.btn-block{:type=>'button',:onclick=>"location.href='#{root_path}';"}='Back'